{
	"manifest_version": 2,
	"name": "Scalebranding RIS",
	"description": "Quickly reverse search images using Google",
	"version": "1.0",
	"minimum_chrome_version": "22",
	"icons": {
		"16": "assets/images/icon16.png",
		"32": "assets/images/icon32.png"
	},
	"permissions": [
		"contextMenus",
		"storage",
		"<all_urls>"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"assets/js/background.js"
		]
	}
}