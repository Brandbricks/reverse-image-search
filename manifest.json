{
	"manifest_version": 3,
	"name": "Google Image Search",
	"description": "Quickly reverse search images using Google",
	"version": "1.0",
	"minimum_chrome_version": "88",
	"icons": {
		"16": "assets/images/icon16.png",
		"32": "assets/images/icon32.png"
	},
	"permissions": [
		"contextMenus"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"background": {
		"service_worker": "assets/js/background.js"
	}
}